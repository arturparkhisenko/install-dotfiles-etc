"*":
  welcome:
    showOnStartup: false
  editor:
    fontSize: 17
    lineHeight: 1.1
    scrollPastEnd: true
    showIndentGuide: true
    softWrap: true
    softWrapAtPreferredLineLength: true
    tabType: "soft"
    zoomFontWhenCtrlScrolling: false
  "exception-reporting":
    userId: "9295d08c-0c7b-383a-085b-88929d08cf02"
  core:
    audioBeep: false
    disabledPackages: [
      "turbo-javascript"
      "vim-mode"
      "polymer-atom"
    ]
    themes: [
      "seti-ui"
      "solarized-dark-syntax"
    ]
  "atom-beautify":
    css:
      convert_quotes: "double"
      default_beautifier: "Pretty Diff"
      force_indentation: true
      newline_between_rules: true
      selector_separator_newline: true
    general:
      _analyticsUserId: "846a2c51-e063-4159-8fdb-5da28c9d1cc6"
      beautifyEntireFileOnSave: false
  "atom-css-comb":
    readyMadeConfigs: "zen"
  "atom-ternjs":
    useSnippets: true
  linter:
    errorPanelHeight: 25
  "linter-eslint":
    disableWhenNoEslintConfig: false
    lintHtmlFiles: true
  minimap:
    plugins:
      "git-diff": true
      "git-diffDecorationsZIndex": 0
      "highlight-selected": true
      linter: true
      linterDecorationsZIndex: 0
      selection: true
  "seti-ui":
    compactView: true
    font: "Default Atom Font"
